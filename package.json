{
  "name": "presenter-export",
  "version": "0.3.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "presenter-export",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link",
    "combine": "node combine.js"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2",
    "sketch-utils": "^0.1.2"
  },
  "author": "Nathan Herald <me@nathanherald.com>",
  "dependencies": {
    "pdfkit": "^0.8.3"
  }
}
